    "function butan(isok,ng,ok)\n",
    "  while abs(ok - ng) > 1\n",
    "    mid = (left + right) ÷ 2\n",
    "    if isok(mid) ok = mid\n",
    "    else ng = mid end\n",
    "  return ok\n",
    "end"
